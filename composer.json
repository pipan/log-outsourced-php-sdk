{
    "name": "outsourced/log-sdk",
    "type": "library",
    "version": "0.1.2",
    "description": "PHP SDK for Log Outsourced API",
    "keywords": [
        "log",
        "logging",
        "library",
        "outsource"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/psr7": "^1.6",
        "psr/http-client": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LogOutsourcedSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
